\mode<presentation>

% Settings
\definecolor{upmblue}{RGB}{104,140,190}
\definecolor{upmdarkblue}{RGB}{61,99,144}
\definecolor{upmgray}{RGB}{210, 221, 239}

\setbeamercolor*{title page header}{fg=upmblue,bg=yellow}
\setbeamercolor*{author}{fg=black,bg=red}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{frametitle continuation}{fg=white}
\setbeamercolor*{item}{fg=upmblue}
\setbeamercolor*{navigation symbols}{bg=upmblue}

\setbeamertemplate{items}[square]
\setbeamertemplate{sections/subsections in toc}[square]


% Blocks for theorems
\setbeamercolor{block title}{use=structure,fg=white,bg=upmdarkblue}
\setbeamercolor{block body}{use=structure,fg=black,bg=upmblue!10}

\mode
<all>
